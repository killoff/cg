<config>
    <acl>
        <resources>
            <admin>
                <children>
                    <customer>
                        <children>
                            <cg_import translate="title">
                                <title>Import Customers</title>
                                <sort_order>200</sort_order>
                            </cg_import>
                        </children>
                    </customer>
                </children>
            </admin>
        </resources>
    </acl>
    <menu>
        <customer>
            <children>
                <cg_import translate="title" module="cg_forms">
                    <title>Import Customers</title>
                    <action>adminhtml/forms/import</action>
                    <sort_order>200</sort_order>
                    <depends>
                        <!--<config>cg_forms/general/is_enabled</config>-->
                    </depends>
                </cg_import>
            </children>
        </customer>
    </menu>
</config>
